{
  "name": "contributte/psr6-caching",
  "description": "PSR-6 (Caching Interface) adapter for Nette Caching",
  "keywords": [
    "nette",
    "psr6",
    "adapter",
    "caching"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/psr6-caching",
  "authors": [
    {
      "name": "Filip Šuška",
      "homepage": "https://github.com/vody105"
    },
    {
      "name": "Milan Felix Šulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=8.0",
    "nette/caching": "^3.0.0",
    "psr/cache": "^1.0.0 || ^2.0.0 || ^3.0.0"
  },
  "require-dev": {
    "nette/di": "^3.0.0",
    "contributte/qa": "^0.4",
    "contributte/tester": "^0.3",
    "contributte/phpstan": "^0.1"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\Psr6\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Contributte\\Psr6\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.3.x-dev"
    }
  }
}
